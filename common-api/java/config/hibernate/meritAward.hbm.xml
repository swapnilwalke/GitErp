<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.virtusa.akura.api.dto.MeritAward" table="MERIT_AWARD">
        <id name="meritAwardsId" type="int" column="MERIT_AWARDS_ID">
            <generator class="increment" />
        </id>
        <property name="awardDescription" type="string">
            <column name="AWARD_DESCRIPTION" length="100" not-null="true" />
        </property>
        <property name="modifiedTime" type="timestamp">
            <column name="MODIFIED_TIME" length="19" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
