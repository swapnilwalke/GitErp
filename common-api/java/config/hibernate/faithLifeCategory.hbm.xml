<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.virtusa.akura.api.dto.FaithLifeCategory" table="FAITH_LIFE_CATEGORY" lazy="false">
        <id name="faithLifeCategoryId" type="int">
            <column name="FAITH_LIFE_CATEGORY_ID" />
            <generator class="increment" />
        </id>
        <property name="description" type="string" column="DESCRIPTION" not-null="true" />
        <property name="modifiedTime" type="timestamp" column="MODIFIED_TIME" not-null="true"
		generated="always"/>
     </class>

</hibernate-mapping>

