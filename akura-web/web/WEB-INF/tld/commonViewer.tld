<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.0</jsp-version>
    <uri>/WEB-INF/tags/</uri>
    <function>
        <name>getDate</name>
        <function-class>com.virtusa.akura.util.HeaderTabConfigurator</function-class>
        <function-signature>
            java.lang.String getDate()
        </function-signature>
    </function>

    <function>
    	<name>getTabContent</name>
    	<function-class>com.virtusa.akura.util.HeaderTabConfigurator</function-class>
    	<function-signature>
    	java.lang.String getTabContent(java.util.PropertyResourceBundle,java.lang.String)
    	</function-signature>
    </function>

    <function>
    	<name>getTabs</name>
    	<function-class>com.virtusa.akura.util.HeaderTabConfigurator</function-class>
    	<function-signature>
    	java.util.List getTabs(java.util.Map,java.lang.String)
    	</function-signature>
    </function>
</taglib>
